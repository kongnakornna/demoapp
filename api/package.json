{
  "name": "cmon-api",
  "version": "0.0.1",
  "description": "Cmon",
  "author": "Cmon IoT",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production  node dist/src/main", 
    "start:staging": "cross-env NODE_ENV=staging nest start --watch", 
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "lint": "eslint ./src -c .eslintrc.json --max-warnings=0",
    "lint:fix": "eslint ./src -c .eslintrc.json --fix",
    "typeorm": "typeorm-ts-node-commonjs -d src/config/db/orm.config.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "migration:generate": "npm run typeorm migration:generate ./migrations/$npm_config_key",
    "migration:create": "typeorm migration:create ./migrations/$npm_config_key",
    "migration:show": "npm run typeorm migration:show",
    "prepare": "husky install",
    "heroku-postbuild": "npm run build --only=dev"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "@influxdata/influxdb-client-apis": "^1.35.0",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.13",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.13",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.13",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/typeorm": "^11.0.0",
    "argon2": "^0.41.1",
    "axios": "^1.8.4",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.1",
    "cache-manager-ioredis": "^2.1.0",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^5.1.5",
    "check-password-strength": "^3.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^2.29.3",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "fs": "^0.0.1-security",
    "helmet": "^6.0.0",
    "influx-backup": "^1.0.1",
    "ioredis": "^5.6.0",
    "ioredis-timeout": "^1.5.0",
    "joi": "^17.6.2",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^2.1.4",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.48",
    "mongoose": "^8.13.2",
    "nestjs-paginate": "^12.1.0",
    "nestjs-typeorm-paginate": "^4.1.0",
    "nodemailer": "^6.10.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.14.1",
    "pug": "^3.0.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "sqlite3orm": "^2.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "util": "^0.12.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^11.0.12",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "i18n": "0.15.1",
    "i18n-express": "1.1.3",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn lint:fix"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
